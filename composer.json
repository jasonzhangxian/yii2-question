{
  "name": "yuncms/yii2-question",
  "description": "The Yii Questions and Answers module inspired by Stack Overflow",
  "keywords": [
    "yii",
    "yii-question",
    "question",
    "question answer",
    "artkost"
  ],
  "type": "yii2-extension",
  "license": "BSD-3-Clause",
  "support": {
    "issues": "https://github.com/yuncms/yii2-question/issues",
    "source": "https://github.com/yuncms/yii2-question"
  },
  "require": {
    "yiisoft/yii2": "~2.0.6",
    "yiisoft/yii2-jui": "~2.0.0",
    "xutl/yii2-select2-widget": "~1.0",
    "yuncms/yii2-coin": "~1.0",
    "yuncms/yii2-tag": "~2.0",
    "yuncms/yii2-user": "~2.0",
    "yuncms/yii2-attachment": "~2.0.0",
    "yuncms/yii2-comment": "~2.0.0",
    "yuncms/yii2-summernote-widget": "~1.0"
  },
  "autoload": {
    "psr-4": {
      "yuncms\\question\\": ""
    }
  },
  "extra": {
    "yuncms": {
      "name": "question",
      "i18n": {
        "class": "yii\\i18n\\PhpMessageSource",
        "basePath": "@yuncms/question/message"
      },
      "migrationNamespace": "@yuncms/question/migrations",
      "backend": {
        "class": "yuncms\\question\\backend\\Module"
      },
      "frontend": {
        "class": "yuncms\\question\\frontend\\Module"
      }
    },
    "branch-alias": {
      "dev-master": "2.0-dev"
    }
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://asset-packagist.org"
    },
    {
      "type": "composer",
      "url": "https://packagist.phpcomposer.com"
    }
  ]
}
